<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rugstats">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register"></activity>
        <activity android:name=".Launcher" />
        <activity android:name=".Popup"
            android:theme = "@style/AppTheme.CustomTheme" />
        <activity android:name=".Popup2"
            android:theme = "@style/AppTheme.CustomTheme2" />
        <activity android:name=".Popup3"
            android:theme = "@style/AppTheme.CustomTheme3" />
        <activity android:name=".Popup4"
            android:theme = "@style/AppTheme.CustomTheme5" />
        <activity android:name=".PopupMatch"
            android:theme = "@style/AppTheme.CustomTheme4" />
        <activity android:name=".ViewPopup"
            android:theme = "@style/AppTheme.CustomTheme5" />
        <activity android:name=".ViewPopup2"
            android:theme = "@style/AppTheme.CustomTheme6" />
        <activity android:name=".ViewPopup3"
            android:theme = "@style/AppTheme.CustomTheme7" />
        <activity android:name=".ViewPopup4"
            android:theme = "@style/AppTheme.CustomTheme8" />

        <activity android:name=".PitchEvent" />
        <activity android:name=".Event" />
        <activity android:name=".ViewStats" />
        <activity android:name=".SelectTeam" />
        <activity android:name=".Timeline" />
        <activity android:name=".SelectMatch" />
        <activity android:name=".VisualStats" />
        <activity android:name=".Login" />

        <activity android:name=".CreateTeam" />
        <activity android:name=".MainActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
          


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>